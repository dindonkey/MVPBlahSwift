  pod 'RxBlocking', '~> 3.0'
  pod 'RxTest', '~> 3.0'
  pod 'RxCocoa', '~> 3.0'
  pod 'MockWebServer'
end

target 'MVPBlahUITests' do
  pod 'RxBlocking', '~> 3.0'
  pod 'RxTest', '~> 3.0'
  pod 'RxCocoa', '~> 3.0'
  pod 'MockWebServer'
end
